{
	"name": "haxeui",
	"displayName": "HaxeUI",
	"version": "0.0.1",
	"publisher": "ibilon",
	"engines": {
		"vscode": "^1.10.0"
	},
	"license": "MIT",
	"description": "HaxeUI support",
	"categories": [
		"Other"
	],
	"icon": "logo.png",
	"keywords": [
		"haxe",
		"haxeui",
		"preview"
	],
	"bugs": "https://github.com/haxeui/haxeui-vscode/issues",
	"homepage": "https://github.com/haxeui/haxeui-vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/haxeui/haxeui-vscode.git"
	},
	"main": "bin/main.js",
	"activationEvents": [
		"onCommand:haxeui.initproject",
		"onCommand:haxeui.show"
	],
	"scripts": {
		"vscode:prepublish": "haxe build.hxml"
	},
	"contributes": {
		"commands": [
			{
				"title": "Init project",
				"command": "haxeui.initproject",
				"category": "HaxeUI"
			},
			{
				"title": "Open preview",
				"command": "haxeui.show",
				"category": "HaxeUI"
			}
		]
	}
}
